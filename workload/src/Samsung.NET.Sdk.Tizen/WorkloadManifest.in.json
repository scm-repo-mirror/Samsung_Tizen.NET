{
  "version": "@TIZEN_WORKLOAD_VERSION@",
  "workloads": {
    "tizen": {
      "description": ".NET SDK Workload for building Tizen applications.",
      "packs": [
        "Samsung.Tizen.Sdk",
        "Samsung.Tizen.Ref.API11",
        "Samsung.Tizen.Ref.API12",
        "Samsung.Tizen.Ref.API13",
        "Samsung.Tizen.Templates",
        "Samsung.NETCore.App.Runtime.tizen"
      ]
    }
  },
  "packs": {
    "Samsung.Tizen.Sdk": {
      "kind": "sdk",
      "version": "@TIZEN_WORKLOAD_VERSION@"
    },
    "Samsung.Tizen.Ref.API11": {
      "kind": "framework",
      "version": "@TizenFXAPI11Version@"
    },
    "Samsung.Tizen.Ref.API12": {
      "kind": "framework",
      "version": "@TizenFXAPI12Version@"
    },
    "Samsung.Tizen.Ref.API13": {
      "kind": "framework",
      "version": "@TizenFXAPI13Version@"
    },
    "Samsung.Tizen.Templates": {
      "kind": "template",
      "version": "@TIZEN_WORKLOAD_VERSION@"
    },
    "Samsung.NETCore.App.Runtime.tizen": {
      "kind": "framework",
      "version": "@TIZEN_WORKLOAD_VERSION@"
    }
  }
}
