<Project>
  <!--
    Workload version:
    * The first two numbers represent the major and minor version of the Tizen OS.
    * The third number is the workload release count.
    * However, the first digit of the third number starts at 101.

    Pre-release version:
    1) CI Versions
       format: -ci.[branch name | gh + #PR].[Commit Distance]+sha[Commit Hash]
       example: -ci.main.100+sha12345
                -ci.gh23.100+sha12345
    2) Official Versions:
       format: -[preview name].[Commit Distance]+sha[Commit Hash]
       example: -preview.3.100+sha12345
  -->
  <PropertyGroup>
    <TizenWorkloadVersion>10.0.123</TizenWorkloadVersion>
  </PropertyGroup>

  <!-- 
    TizenFX API version:
    * The first number represents the API Level.
    * The sencond and third numbers represents the reserved version.
    * The fourth number represents the build number.
    This version is used to generate Samsung.Tizen.Ref.APIXX package.
  -->
  <PropertyGroup>
    <TizenFXAPI11Version>11.0.0.18033</TizenFXAPI11Version>
    <TizenFXAPI12Version>12.0.0.18510</TizenFXAPI12Version>
    <TizenFXAPI13Version>13.0.0.19231</TizenFXAPI13Version>
  </PropertyGroup>

  <!--
    The default .NET SDK version to be used when executing make command without 'DOTNET_VERSION' parameter input.
  -->
  <PropertyGroup>
    <MicrosoftDotnetSdkInternalPackageVersion>10.0.100</MicrosoftDotnetSdkInternalPackageVersion>
  </PropertyGroup>

  <!--
    Dotnet SDK versions for building the workload.
   -->
  <PropertyGroup Condition="$(MicrosoftDotnetSdkInternalPackageVersion.StartsWith('8.0'))">
    <MicrosoftDotNetBuildTasksFeedPackageVersion>8.0.0-beta.24360.5</MicrosoftDotNetBuildTasksFeedPackageVersion>
  </PropertyGroup>

  <PropertyGroup Condition="$(MicrosoftDotnetSdkInternalPackageVersion.StartsWith('9.0'))">
    <MicrosoftDotNetBuildTasksFeedPackageVersion>9.0.0-beta.25065.2</MicrosoftDotNetBuildTasksFeedPackageVersion>
  </PropertyGroup>

  <PropertyGroup Condition="$(MicrosoftDotnetSdkInternalPackageVersion.StartsWith('10.0'))">
    <MicrosoftDotNetBuildTasksFeedPackageVersion>10.0.0-beta.25531.102</MicrosoftDotNetBuildTasksFeedPackageVersion>
  </PropertyGroup>
</Project>
